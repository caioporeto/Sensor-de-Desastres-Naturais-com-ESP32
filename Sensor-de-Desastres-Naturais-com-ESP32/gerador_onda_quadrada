#define OUTPUT_PIN 17
#define FREQ 125000
#define PWM_CHANNEL 0
#define RESOLUTION 8

void setup() {
  Serial.begin(115200);
  bool sucess = ledcAttach(OUTPUT_PIN, FREQ, RESOLUTION);
  if (!sucess) {
    Serial.println(" Falha ao configurar o PWM ");
  }
  ledcWrite(OUTPUT_PIN, 128); // duty cicle de 50% (128 Ã© metade de 256 [2^8])
  Serial.println(" Onda quadrada de 125 kHz gerada! ");
}

void loop() {
  //
}
